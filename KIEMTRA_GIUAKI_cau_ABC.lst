....................          Y = XA|ZA;         
....................          XUAT_32LED_DON_1DW(Y); 
....................          DELAY_CHON(DL,CHONDELAY); 
....................          JA--; 
....................       } 
....................       ELSE  
....................       { 
....................          IA++; 
....................          JA=IA;       
....................          YA = YA<<1; 
....................          XA = XA <<1; 
....................          ZA = YA; 
....................       } 
....................    } 
....................    ELSE RESET_TOAN_BO_BIEN(); 
