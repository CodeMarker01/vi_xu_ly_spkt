.................... {         
....................       IF (TGDLAY==1)          XUAT_32LED_DON_1DW(Y);  
....................       ELSE IF (TGDLAY==50)    DAO_DL_32BIT_VA_XUAT(); 
....................       ELSE IF (TGDLAY==100)   DAO_DL_32BIT_VA_XUAT();           
....................       ELSE IF (TGDLAY==150)   DAO_DL_32BIT_VA_XUAT();                  
