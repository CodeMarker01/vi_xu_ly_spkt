#include <tv_pickit2_shift_1.c>                                               
#include <tv_pickit2_shift_key4x4_138.c>                     

signed INT8 mp;                                                   
unsigned int8 y;

void main()
{                                                                  
   set_up_port_ic_chot();
   set_tris_b(0x3c);
   xuat_32led_don_4byte(0,0,0,mp); 

   WHILE(true)                                
   {                                                
      mp = key_4x4_dw();
      IF (mp != 0) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 1) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 2) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 3) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 4) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 5) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 6) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 7) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 8) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 9) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 10) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 11) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 12) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 13) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 14) xuat_32led_don_4byte(0,0,0,mp);
      IF (mp != 15) xuat_32led_don_4byte(0,0,0,mp);
                                          
   }                                    
}
                       
