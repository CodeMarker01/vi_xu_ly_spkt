.................... VOID DIEM_SANG_DICH_TRAI_MAT_DAN_32LED(UNSIGNED INT8 DL,CHONDELAY) 
.................... {        
....................    IF(IA<33) 
....................    { 
....................       IF(JA>0) 
....................       { 
....................          ZB = (ZB << 1); 
....................          Y = XB|ZB;         
....................          XUAT_32LED_DON_1DW(Y); 
....................          DELAY_CHON(DL,CHONDELAY); 
....................          JA--; 
....................       } 
....................       ELSE  
....................       { 
....................          IA++; 
....................          JA=IA;       
....................          YB = YB>>1; 
....................          XB = XB>>1; 
....................          ZB = YB; 
....................       } 
....................    } 
....................    ELSE RESET_TOAN_BO_BIEN(); 
.................... }   
