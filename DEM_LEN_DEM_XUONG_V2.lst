....................  
.................... typedef union  
.................... {  
....................   unsigned int16 word;  
....................   unsigned int8 nbyte[2];  
.................... } Dots;  
....................  
.................... typedef struct  
.................... {  
....................   int1 refresh;  
....................   Dots PIXEL[GLCD_DOC][GLCD_NGANG];     
....................   } GD_RAM;     
