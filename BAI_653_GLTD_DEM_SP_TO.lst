.................... void glcd_pixel(int8 x, int8 y, int1 color)  
.................... {  
....................   int8  b;  
....................   if(y>31){x += 128; y-= 32;};  
....................   DOC = y;  
....................   NGANG = x/16;  
....................   b = 15 - (x % 16);  
....................    
....................   if (color == 1) bit_set (gdram_vdk.PIXEL[DOC][NGANG].word, b);  
....................   else bit_clear (gdram_vdk.PIXEL[DOC][NGANG].word, b);  
....................   gdram_vdk.refresh = TRUE;  
.................... }  
